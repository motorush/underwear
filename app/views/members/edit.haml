.page-wrap
  - form_for :user do |f|
    %p
    = f.label :email
    %br
    = f.text_field :email
    %p
    = f.label :change_password
    %br
    =f.password_field :password
    %p
    = f.label :password_confirmation
    %br
    =f.password_field :password_confirmation

    %p= f.submit "update"

